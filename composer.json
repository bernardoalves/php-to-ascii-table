{
    "name": "malios/php-to-ascii-table",
    "description": "A PHP library to generate plain text tables.",
    "type": "library",
    "keywords": [
        "php",
        "table",
        "ascii",
        "plain text table"
    ],
    "authors": [
        {
            "name": "Mehmed Aliosman",
            "email": "dev.mehmed.aliosman@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7",
        "php-ds/php-ds": "^1.1",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "phpstan/phpstan": "^0.6.4"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "AsciiTable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AsciiTable\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpstan"
        ],
        "phpunit": "phpunit tests/ colors=always",
        "phpstan": "phpstan analyse src/ -l 5"
    }
}
